{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "back-end\\front-end",
      "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|css|js)",
        "headers" : [ {
            "key" : "Cache-Control",
            "value" : "max-age=1000000"
        } ]
      }
    ]
  }
}
